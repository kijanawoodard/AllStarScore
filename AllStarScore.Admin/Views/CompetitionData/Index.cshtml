@model AllStarScore.Admin.ViewModels.CompetitionDataIndexViewModel

@{ Html.RegisterScript("~/content/scripts/competitionData.index.js"); }

@using(Html.BeginScript(ScriptPositionEnum.EndOfPage)) { 
	<text>
		$(document).ready(function () { 
		    var competitionData = JSON.parse(@(Html.EncodeAsJson(Model)));
	        window.AllStarScore.CompetitionData = new window.AllStarScore.CompetitionData.ViewModel(competitionData);
            window.AllStarScore.ScoringMap = new window.AllStarScore.ScoringMap(competitionData.scoringMap);
		});
	</text>
}