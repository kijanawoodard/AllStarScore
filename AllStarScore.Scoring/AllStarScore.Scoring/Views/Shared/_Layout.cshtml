<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - AllStar Scoring</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
@*        <link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.21/themes/base/jquery-ui.css" media="all" />*@
        @{ Html.RegisterStylesheet("~/content/themes/base/jquery-ui.css", "global"); }
        @{ Html.RegisterStylesheet("~/content/styles/normalize.css", "global"); }
        @{ Html.RegisterStylesheet("~/content/styles/jquery-ui-timepicker-addon.css", "global"); }
@*        @{ Html.RegisterStylesheet("~/content/styles/tablesorter.themes/blue/style.css", "global"); }*@
        
        @*my stuff below*@
        @{ Html.RegisterStylesheet("~/content/styles/site.css", "global"); }
        @{ Html.RegisterStylesheet("~/content/styles/global.css", "global"); }
        <link rel="stylesheet" href="~/content/styles/site.mobile.css" media="all" />

        @Html.RenderCss()
        <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    </head>
    <body>
        <header>
            <div class="content-wrapper">
@*                <div class="float-left">*@
@*                    <p class="site-title">@Html.ActionLink("your logo here.", "Index", "Home")</p>*@
@*                </div>*@
@*                <div class="float-right">*@
@*                    <section id="login">*@
@*$1$                        @Html.Partial("_LoginPartial")#1#*@
@*                    </section>*@
@*                    <nav>*@
@*                        <ul id="menu">*@
@*                            <li>@Html.ActionLink("Home", "Index", "Home")</li>*@
@*                            <li>@Html.ActionLink("About", "About", "Home")</li>*@
@*                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
@*                            <li>@Html.ActionLink("Competitions", "Index", "Competition")</li>*@
@*                        </ul>*@
@*                    </nav>*@
@*                </div>*@
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - Executive Closing, Inc.</p>
                </div>
            </div>
        </footer>
