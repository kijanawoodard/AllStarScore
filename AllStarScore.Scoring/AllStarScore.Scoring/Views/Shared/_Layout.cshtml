        @{ Html.RegisterScript("~/content/scripts/lib/jquery-1.7.2.min.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/jquery-ui-1.8.20.min.js", "global"); }
        
        @{ Html.RegisterScript("~/content/scripts/lib/jquery.validate.min.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/knockout-2.1.0.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/knockout.mapping-latest.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/knockout-sortable.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/jquery.tablesorter.min.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/date.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/jquery-ui-timepicker-addon.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/underscore-min.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/lib/jquery.cookie.js", "global"); }
        
        @*my stuff below*@
        @{ Html.RegisterScript("~/content/scripts/global.knockout.bindings.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/global.tinypubsub.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/global.ajax.js", "global"); }
        @{ Html.RegisterScript("~/content/scripts/global.datepicker.js", "global"); }
        
        @Html.RenderScripts()
    </body>
</html>
